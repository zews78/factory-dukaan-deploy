    "@babel/core": "^7.24.0",
    "eslint": "^7.4.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
